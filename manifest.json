{
  "manifest_version": 2,
  "name": "Refined Medium",
  "version": "0.1.4",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "on.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "run_at": "document_end",
    "js": ["content.js"]
  }],
  "web_accessible_resources": ["styles.css"]
}